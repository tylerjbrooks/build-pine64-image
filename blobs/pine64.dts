/dts-v1/;

/memreserve/ 0x45000000 0x200000;
/memreserve/ 0x41010000 0x10000;
/memreserve/ 0x41020000 0x800;
/memreserve/ 0x40100000 0x4000;
/memreserve/ 0x40104000 0x1000;
/memreserve/ 0x40105000 0x1000;

/{
    model = "sun50iw1p1";
    compatible = "arm,sun50iw1p1", "arm,sun50iw1p1";
    interrupt-parent = <0x01>;
    #address-cells = <0x02>;
    #size-cells = <0x02>;

    aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		serial4 = &uart4;
        twi0 = &twi0;
		s_twi0 = &s_twi0;
        twi1 = &twi1;
        twi2 = &twi2;
        spi0 = &spi0;
        spi1 = &spi1;
        global_timer0 = &soc_timer0;
        cci0 = &csi_cci0;
        csi_res0 = &csi_res0;
        vfe0 = &csi0;
        mmc0 = &sdc0;
        mmc2 = &sdc2;
        nand0 = &nand0;
        disp = &disp;
        lcd0 = &lcd0;
        hdmi = &hdmi;
        pwm = &pwm;
        pwm0 = &pwm0;
        s_pwm = &s_pwm; 
        spwm0 = &spwm0; 
        boot_disp = &boot_disp; 
    };

    chosen {
        bootargs = "earlyprintk=sunxi-uart,0x01c28000 loglevel=8 initcall_debug=1 console=ttyS0 init=/init";
        linux,initrd-start = <0x00 0x00>;
        linux,initrd-end = <0x00 0x00>;
    };

    cpus {
        #address-cells = <0x02>;
        #size-cells = <0x00>;

        cpu@0 {
            device_type = "cpu";
            compatible = "arm,cortex-a53", "arm,armv8";
            reg = <0x00 0x00>;
            enable-method = "psci";
            cpufreq_tbl = <0x00075300 0x000927c0 0x000afc80 
				0x000c7380 0x000dea80 0x000ea600 
				0x000f6180 0x00101d00 0x0010d880 
				0x00119400 0x00124f80 0x00148200>;
            clock-latency = <0x001e8480>;
            clock-frequency = <0x3c14dc00>;
            cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0 &SYS_SLEEP_0>;
        };
        cpu@1 {
            device_type = "cpu";
            compatible = "arm,cortex-a53", "arm,armv8";
            reg = <0x00 0x01>;
            enable-method = "psci";
            clock-frequency = <0x3c14dc00>;
            cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0 &SYS_SLEEP_0>;
        };
        cpu@2 {
            device_type = "cpu";
            compatible = "arm,cortex-a53", "arm,armv8";
            reg = <0x00 0x02>;
            enable-method = "psci";
            clock-frequency = <0x3c14dc00>;
            cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0 &SYS_SLEEP_0>;
        };
        cpu@3 {
            device_type = "cpu";
            compatible = "arm,cortex-a53", "arm,armv8";
            reg = <0x00 0x03>;
            enable-method = "psci";
            clock-frequency = <0x3c14dc00>;
            cpu-idle-states = <&CPU_SLEEP_0 &CLUSTER_SLEEP_0 &SYS_SLEEP_0>;
        };
        idle-states {
            entry-method = "arm,psci";
            CPU_SLEEP_0: cpu-sleep-0 {
                compatible = "arm,idle-state";
                arm,psci-suspend-param = <0x00010000>;
                entry-latency-us = <0x28>;
                exit-latency-us = <0x64>;
                min-residency-us = <0x96>;
            };
            CLUSTER_SLEEP_0: cluster-sleep-0 {
                compatible = "arm,idle-state";
                arm,psci-suspend-param = <0x01010000>;
                entry-latency-us = <0x01f4>;
                exit-latency-us = <0x03e8>;
                min-residency-us = <0x09c4>;
           };
            SYS_SLEEP_0: sys-sleep-0 {
                compatible = "arm,idle-state";
                arm,psci-suspend-param = <0x02010000>;
                entry-latency-us = <0x03e8>;
                exit-latency-us = <0x07d0>;
                min-residency-us = <0x1194>;
            };
        };
    };
    psci {
        compatible = "arm,psci-0.2";
        method = "smc";
        psci_version = <0x84000000>;
        cpu_suspend = <0xc4000001>;
        cpu_off = <0x84000002>;
        cpu_on = <0xc4000003>;
        affinity_info = <0xc4000004>;
        migrate = <0xc4000005>;
        migrate_info_type = <0x84000006>;
        migrate_info_up_cpu = <0xc4000007>;
        system_off = <0x84000008>;
        system_reset = <0x84000009>;
    };
    n_brom {
        compatible = "allwinner,n-brom";
        reg = <0x00 0x00 0x00 0xc000>;
    };
    s_brom {
        compatible = "allwinner,s-brom";
        reg = <0x00 0x00 0x00 0x00010000>;
    };
    sram_a1 {
        compatible = "allwinner,sram_a1";
        reg = <0x00 0x00010000 0x00 0x8000>;
    };
    sram_a2 {
        compatible = "allwinner,sram_a2";
        reg = <0x00 0x00040000 0x00 0x00014000>;
    };
    prcm {
        compatible = "allwinner,prcm";
        reg = <0x00 0x01f01400 0x00 0x0400>;
    };
    cpuscfg {
        compatible = "allwinner,cpuscfg";
        reg = <0x00 0x01f01c00 0x00 0x0400>;
    };
    ion {
        compatible = "allwinner,sunxi-ion";
        system_contig {
            type = <0x01>;
        };
        cma {
            type = <0x04>;
        };
        system {
            type = <0x00>;
        };
    };
    dram {
        compatible = "allwinner,dram";
        clocks = <&clk_pll_ddr0>, <&clk_pll_ddr1>;
        clock-names = "pll_ddr0", "pll_ddr1";
        dram_clk = <0x02a0>;
        dram_type = <0x03>;
        dram_zq = <0x003b3bdd>;
        dram_odt_en = <0x01>;
        dram_para1 = <0x10e40400>;
        dram_para2 = <0x04000000>;
        dram_mr0 = <0x1c70>;
        dram_mr1 = <0x40>;
        dram_mr2 = <0x18>;
        dram_mr3 = <0x00>;
        dram_tpr0 = <0x0048a192>;
        dram_tpr1 = <0x01c2418d>;
        dram_tpr2 = <0x00076051>;
        dram_tpr3 = <0x050005dc>;
        dram_tpr4 = <0x00>;
        dram_tpr5 = <0x00>;
        dram_tpr6 = <0x00>;
        dram_tpr7 = <0x2a066198>;
        dram_tpr8 = <0x00>;
        dram_tpr9 = <0x00>;
        dram_tpr10 = <0x8808>;
        dram_tpr11 = <0x40a60066>;
        dram_tpr12 = <0x55550000>;
        dram_tpr13 = <0x04000903>;
        device_type = "dram";
    };
    memory@40000000 {
        device_type = "memory";
        reg = <0x00 0x41000000 0x00 0x3f000000>;
    };
    gic: interrupt-controller@1c81000 {
        compatible = "arm,cortex-a15-gic", "arm,cortex-a9-gic";
        #interrupt-cells = <0x03>;
        #address-cells = <0x00>;
        device_type = "gic";
        interrupt-controller;
        reg = <0x00 0x01c81000 0x00 0x1000 
		0x00 0x01c82000 0x00 0x2000 
		0x00 0x01c84000 0x00 0x2000 
		0x00 0x01c86000 0x00 0x2000>;
        interrupts = <0x01 0x09 0x0f04>;
        linux,phandle = <0x01>;
        phandle = <0x01>;
    };
    chipid: sunxi-chipid@1c14200 {
        compatible = "sunxi,sun50i-chipid";
        device_type = "chipid";
        reg = <0x00 0x01c14200 0x00 0x0400>;
    };
    timer {
        compatible = "arm,armv8-timer";
        interrupts = <0x01 0x0d 0xff01 
			0x01 0x0e 0xff01 
			0x01 0x0b 0xff01 
			0x01 0x0a 0xff01>;
        clock-frequency = <0x016e3600>;
    };
    pmu {
        compatible = "arm,armv8-pmuv3";
        interrupts = <0x00 0x78 0x04 
		0x00 0x79 0x04 
		0x00 0x7a 0x04 
		0x00 0x7b 0x04>;
    };
    dvfs_table: dvfs_table {
        compatible = "allwinner,dvfs_table";
        max_freq = <0x44aa2000>;
        min_freq = <0x1c9c3800>;
        lv_count = <0x08>;
        lv1_freq = <0x44aa2000>;
        lv1_volt = <0x0514>;
        lv2_freq = <0x41cdb400>;
        lv2_volt = <0x04ec>;
        lv3_freq = <0x3ef14800>;
        lv3_volt = <0x04d8>;
        lv4_freq = <0x3c14dc00>;
        lv4_volt = <0x04b0>;
        lv5_freq = <0x39387000>;
        lv5_volt = <0x0488>;
        lv6_freq = <0x365c0400>;
        lv6_volt = <0x0460>;
        lv7_freq = <0x30a32c00>;
        lv7_volt = <0x0438>;
        lv8_freq = <0x269fb200>;
        lv8_volt = <0x0410>;
        device_type = "dvfs_table";
    };
    dramfreq {
        compatible = "allwinner,sunxi-dramfreq";
        reg = <0x00 0x01c62000 0x00 
			0x1000 0x00 0x01c63000 
			0x00 0x1000 0x00 
			0x01c20000 0x00 0x0800>;
        clocks = <&clk_pll_ddr0>, <&clk_pll_ddr1>, <&clk_ahb1>;
        status = "okay";
    };

    uboot: uboot {
    };

    gpu_mali400_0: gpu@0x01c40000 {
        compatible = "arm,mali-400", "arm,mali-utgard";
        reg = <0x00 0x01c40000 0x00 0x00010000>;
        interrupts = <0x00 0x61 0x04 
			0x00 0x62 0x04 
			0x00 0x63 0x04 
			0x00 0x64 0x04 
			0x00 0x66 0x04 
			0x00 0x67 0x04>;
        interrupt-names = "IRQGP", "IRQGPMMU", "IRQPP0", 
			"IRQPPMMU0", "IRQPP1", "IRQPPMMU1";
        clocks = <&clk_pll_gpu>, <&clk_gpu>;
        device_type = "gpu_mali400_0";
        normal_freq = <0x0198>;
        scene_ctrl_status = <0x00>;
        temp_ctrl_status = <0x01>;
    };
    wlan: wlan {
        compatible = "allwinner,sunxi-wlan";
        wlan_io_regulator = "vcc-wifi-io";
        wlan_busnum = <0x01>;
        status = "okay";
        device_type = "wlan";
        clocks;
        wlan_power;
        wlan_regon = <&r_pio 0x0b 0x02 0x01 0xffffffff 0xffffffff 0x00>;
        wlan_hostwake = <&r_pio 0x0b 0x03 0x06 0xffffffff 0xffffffff 0x00>;
        efuse_map_path = "wifi_efuse_8189e_for_MB1019Q5.map";
    };
    bt: bt {
        compatible = "allwinner,sunxi-bt";
        bt_io_regulator = "vcc-wifi-io";
        status = "okay";
        device_type = "bt";
        clocks;
        bt_power;
        bt_rst_n = <&r_pio 0x0b 0x04 0x01 0xffffffff 0xffffffff 0x00>;
    };
    btlpm: btlpm {
        compatible = "allwinner,sunxi-btlpm";
        uart_index = <0x01>;
        status = "okay";
        device_type = "btlpm";
        bt_wake = <&r_pio 0x0b 0x06 0x01 0xffffffff 0xffffffff 0x01>;
        bt_hostwake = <&r_pio 0x0b 0x05 0x06 0xffffffff 0xffffffff 0x00>;
    };

	/*** Clocks configuration  ***/
    clocks {
        compatible = "allwinner,sunxi-clk-init";
        device_type = "clocks";
        #address-cells = <0x02>;
        #size-cells = <0x02>;
        ranges;
        reg = <0x00 0x01c20000 0x00 
			0x0320 0x00 0x01f01400 
			0x00 0xb0 0x00 
			0x01f00060 0x00 0x04>;

        clk_losc: losc {
            #clock-cells = <0x00>;
            compatible = "allwinner,fixed-clock";
            clock-frequency = <0x8000>;
            clock-output-names = "losc";
            linux,phandle = <0x0c>;
            phandle = <0x0c>;
        };

        clk_iosc: iosc {
            #clock-cells = <0x00>;
            compatible = "allwinner,fixed-clock";
            clock-frequency = <0x00f42400>;
            clock-output-names = "iosc";
            linux,phandle = <0x0d>;
            phandle = <0x0d>;
        };

        clk_hosc: hosc {
            #clock-cells = <0x00>;
            compatible = "allwinner,fixed-clock";
            clock-frequency = <0x016e3600>;
            clock-output-names = "hosc";
            linux,phandle = <0x06>;
            phandle = <0x06>;
        };

        clk_pll_cpu: pll_cpu {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            clock-output-names = "pll_cpu";
        };

        clk_pll_audio: pll_audio {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            assigned-clock-rates = <0x01770000>;
            clock-output-names = "pll_audio";
            linux,phandle = <0x02>;
            phandle = <0x02>;
        };

        clk_pll_video0: pll_video0 {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            assigned-clock-rates = <0x11b3dc40>;
            clock-output-names = "pll_video0";
            linux,phandle = <0x03>;
            phandle = <0x03>;
        };

        clk_pll_ve: pll_ve {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            clock-output-names = "pll_ve";
            linux,phandle = <0x16>;
