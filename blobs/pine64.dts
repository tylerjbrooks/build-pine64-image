            linux,phandle = <0x3a>;
            phandle = <0x3a>;
        };

        clk_pll_de: pll_de {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            assigned-clock-rates = <0x11b3dc40>;
            clock-output-names = "pll_de";
            linux,phandle = <0x07>;
            phandle = <0x07>;
        };

        clk_pll_ddr1: pll_ddr1 {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            clock-output-names = "pll_ddr1";
            linux,phandle = <0x94>;
            phandle = <0x94>;
        };

        clk_pll_audiox8: pll_audiox8 {
            #clock-cells = <0x00>;
            compatible = "allwinner,fixed-factor-clock";
            clocks = <0x02>;
            clock-mult = <0x08>;
            clock-div = <0x01>;
            clock-output-names = "pll_audiox8";
        };

        clk_pll_audiox4: pll_audiox4 {
            #clock-cells = <0x00>;
