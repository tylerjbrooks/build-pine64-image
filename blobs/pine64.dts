            phandle = <0x93>;
        };

        clk_pll_periph0: pll_periph0 {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            clock-output-names = "pll_periph0";
            linux,phandle = <0x04>;
            phandle = <0x04>;
        };

        clk_pll_periph1: pll_periph1 {
            #clock-cells = <0x00>;
            compatible = "allwinner,sunxi-pll-clock";
            lock-mode = "new";
            clock-output-names = "pll_periph1";
            linux,phandle = <0x05>;
